<%- if controller_name != 'sessions' %>
  <%= link_to t('app.log_in'), new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to t('password.forgot'), new_password_path(resource_name) %>
<% end %>
